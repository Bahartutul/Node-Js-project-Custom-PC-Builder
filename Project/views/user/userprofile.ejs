                </a>
                <a href="#" class="list-group-item text-center">
                  <h4 class="glyphicon glyphicon-list-alt"></h4><br/>Order List
                </a>
                <a href="" class="list-group-item text-center">
                  <h4 class="glyphicon glyphicon-envelope"></h4><br/>Message<span class="badge"><%= session.cart.totalQty %></span>
                </a>
                <a href="" class="list-group-item text-center">
                  <h4 class="glyphicon glyphicon-cog"></h4><br/>Change Password
                </a>
              </div>
            </div>
            <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9 bhoechie-tab">
                <!-- flight section -->
                <div class="bhoechie-tab-content active">
                    
  						<div class="container">
  							<div class="row">
  								<label>Name:</label>
  								<%= user[0].name %>
  							</div>
  							<div class="row">
  								<label>Address:</label>
  								<%= user[0].address %>
  							</div>
  							<div class="row">
  								<label>Phone No:</label>
  								<%= user[0].phone %>
  							</div>
  						
  							
  							
  						</div>

                    
                </div>
                <!-- train section -->
                <div class="bhoechie-tab-content">
                    <center>
                      	<div class="container">
                      		<table class="table table-striped" width ="100%">
                      			<tr align="center">
                      				<td><label>Brand</label></td>
                      				<td><label>Model</label></td>
                      				<td><label>Type</label></td>
                      				<td><label>Power Supply</label></td>
									<td><label>Price</label></td>
								</tr>
							</table>
						</div>
                    </center>
                </div>
    
                <!-- hotel search -->
                <div class="bhoechie-tab-content">
                    <center>
                            <div class="container">

                               <div class="row">
                                 <div class="col-sm-6 col-md-6">
                                   <ul class="list-group-item">
                                    <li>Form</li>
                                    <li>Message</li>

                                      
                                   </ul>
                              
                                    
                                  </div>
                               </div>
                    </center>
                </div>
                <div class="bhoechie-tab-content">
                    <center>
                      <h1 class="glyphicon glyphicon-cutlery" style="font-size:12em;color:#55518a"></h1>
                      <h2 style="margin-top: 0;color:#55518a">Cooming Soon</h2>
                      <h3 style="margin-top: 0;color:#55518a">Restaurant Diirectory</h3>
                    </center>
                </div>
                <div class="bhoechie-tab-content">
                    <center>
                      
                      <a href="http:/...remove/logout"> <h1 class="glyphicon glyphicon-log-out" style="font-size:12em;color:#55518a"></h1></a>
                
                    </center>
                </div>
            </div>
        </div>
  </div>
</div>
<script>
	$(document).ready(function() {
    $("div.bhoechie-tab-menu>div.list-group>a").click(function(e) {
        e.preventDefault();
        $(this).siblings('a.active').removeClass("active");
        $(this).addClass("active");
        var index = $(this).index();
        $("div.bhoechie-tab>div.bhoechie-tab-content").removeClass("active");
        $("div.bhoechie-tab>div.bhoechie-tab-content").eq(index).addClass("active");
    });
});
</script>
</body>
</html>